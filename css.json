{
	// --phone: 0px;
  // --tablet_portrait: 576px;
  // --tablet_landscape: 768px;
  // --laptop: 992px;
	// --desktop: 1200px;
	"breakpoints": {
		"prefix": ["breakpoints", "list_breakpoints", "all_breakpoints"],
		"body": [
			"@media (min-width: 0px) {} /* phone */",
			"@media (min-width: 576px) {} /* tablet_portrait */",
			"@media (min-width: 768px) {} /* tablet_landscape */",
			"@media (min-width: 992px) {} /* laptop */",
			"@media (min-width: 1200px) {} /* desktop+ */"
		]
	},
	"breakpoint_phone": {
		"prefix": ["breakpoint_phone", "phone"],
		"body": [
			"@media (min-width: 0px) {",
			"\\t$0",
			"}"
		]
	},
	"breakpoint_tablet_portrait": {
		"prefix": ["breakpoint_tablet_portrait", "tablet_portrait"],
		"body": [
			"@media (min-width: 576px) {",
			"\\t$0",
			"}"
		]
	},
	"breakpoint_tablet": {
		"prefix": ["breakpoint_tablet", "tablet", "breakpoint_tablet_landscape", "tablet_landscape"],
		"body": [
			"@media (min-width: 768px) {",
			"\\t$0",
			"}"
		]
	},
	"breakpoint_laptop": {
		"prefix": ["breakpoint_laptop", "laptop"],
		"body": [
			"@media (min-width: 992px) {",
			"\\t$0",
			"}"
		]
	},
	"breakpoint_desktop": {
		"prefix": ["breakpoint_desktop", "desktop"],
		"body": [
			"@media (min-width: 1200px) {",
			"\\t$0",
			"}"
		]
	},
	"flex_row": {
		"prefix": ["flex_row", "row"],
		"body": [
			"display: flex;",
			"flex-direction: row;"
		]
	},
	"flex_col": {
		"prefix": ["flex_col", "col"],
		"body": [
			"display: flex;",
			"flex-direction: column;"
		]
	},
	"flex_center": {
		"prefix": ["flex_center", "align_center", "center"],
		"body": [
			"align-items: center;"
		]
	},
	"flex_stretch": {
		"prefix": ["flex_stretch", "align_stretch", "stretch"],
		"body": [
			"align-items: stretch;"
		]
	},
	"m_0": {
		"prefix": ["m_0", "m0"],
		"body": [
			"margin: 0;"
		]
	},
	"m_4": {
		"prefix": ["m_4", "m4"],
		"body": [
			"margin: 4px;"
		]
	},
	"m_5": {
		"prefix": ["m_5", "m5"],
		"body": [
			"margin: 5px;"
		]
	},
	"m_8": {
		"prefix": ["m_8", "m8"],
		"body": [
			"margin: 8px;"
		]
	},
	"m_10": {
		"prefix": ["m_10", "m10"],
		"body": [
			"margin: 10px;"
		]
	},
	"margin": {
		"prefix": ["m", "mg", "margin"],
		"body": [
			"margin: ${1:10}px;"
		]
	},
	"p_0": {
		"prefix": ["p_0", "p0"],
		"body": [
			"padding: 0;"
		]
	},
	"p_4": {
		"prefix": ["p_4"],
		"body": [
			"padding: 4px;"
		]
	},
	"p_5": {
		"prefix": ["p_5"],
		"body": [
			"padding: 5px;"
		]
	},
	"p_8": {
		"prefix": ["p_8"],
		"body": [
			"padding: 8px;"
		]
	},
	"p_10": {
		"prefix": ["p_10"],
		"body": [
			"padding: 10px;"
		]
	},
	"padding": {
		"prefix": ["p", "pad", "padding"],
		"body": [
			"padding: ${1:10}px;"
		]
	},
}