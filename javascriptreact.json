	},

	"it": {
		"prefix": "it",
		"body": "it('should ${1}', () => {\n\t\n});"
	},

	"connect": {
		"prefix": "connect",
		"body": "import {connect} from 'react-redux';\n\nconst Connected${1} = connect(state => ({\n\n}), dispatch => ({\n\n}))(${1});\n\nexport default Connected${1};"
	},

