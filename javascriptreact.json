  "joi_schema": {
    "prefix": "joi_schema",
    "description": "",
    "body": [
      "const nodeSchema = Joi.object().keys({",
      "  name: Joi.string().required(),",
      "  props: Joi.object()",
      "    .pattern(Joi.string(), Joi.string())",
      "    .optional(),",
      "  children: Joi.array()",
      "    .items(Joi.object())",
      "    .optional(),",
      "  text: Joi.string().optional()",
      "});"
    ]
  },
