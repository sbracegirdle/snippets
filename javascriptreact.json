    "prefix": "cdk_lambda",
    "body": [
      "import lambda = require('@aws-cdk/aws-lambda');",
      "",
      "const lambdaFn = new lambda.Function(this, 'Singleton', { ",
      "  code: new lambda.InlineCode(fs.readFileSync('lambda-handler.py', { encoding: 'utf-8' })),",
